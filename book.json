{
  "title": "Web学习笔记",
  "author": "wchaochao",
  "description": "前端学习过程中要整理的笔记",
  "language": "zh-hans",
  "styles": {
    "website": "styles/website.css"
  },
  "plugins": [
    "disqus",
    "-lunr",
    "-search",
    "search-pro",
    "github",
    "splitter",
    "local-pagefooter",
    "toggle-chapters",
    "todo"
  ],
  "pluginsConfig": {
    "disqus": {
      "shortName": "https-wchaochao-gitbooks-io-webbook-content"
    },
    "github": {
      "url": "https://github.com/wchaochao/webbook"
    },
    "sharing": {
      "weibo": false,
      "facebook": true,
      "twitter": true,
      "google": false,
      "instapaper": false,
      "vk": false,
      "all": [
        "facebook",
        "google",
        "twitter",
        "weibo",
        "instapaper"
      ]
    },
    "local-pagefooter": {
        "islocal":true,
        "modify_label": "最近更新：",
        "modify_format": "YYYY-MM-DD HH:mm:ss"
    }
  }
}